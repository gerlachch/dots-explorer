
struct Host [substruct_only] {
    1: string endpoint;
    2: string description;
}

struct HostSettings {
    1: vector<Host> hosts;
    2: uint32 selectedHost;
    3: bool autoConnect;
}

struct FilterSettings {
    1: string regexFilter;
    2: bool showInternal;
    3: bool showUncached;
    4: bool showEmpty;
    5: bool matchCase;
}
